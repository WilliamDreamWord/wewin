.container{
  display:flex;
  justify-content:flex-start;
  padding:0;
}
.nav{
  display:position;
  width:100%;
  /* position:relative;
  bottom:50rpx; */
  height:100rpx;
  background-color:rgb(48,146,247);
  color:white;
  text-align:center;
  line-height:100rpx;
  font-size: 45rpx;
}
.header {
  display: position;
  width: 100%;
  font-size: 30rpx;
  margin: center;
}
.list{
  display: flex;
  width:100%;
}
.bottom{
  display: flex;
  width: 100%;
}
.item{
  position: relative;
  width: 100%;
  height: 200rpx;
  overflow: hidden;
  border-bottom: 1px solid rgb(235, 235, 235);
}
.top{
  position:relative;
  width:100%;
  height:240rpx;
  border-bottom:1px solid #ccc;
  background-color:white;
}
.logo{
  position:absolute;
  left:30rpx;
  top:50%;
  transform: translateY(-50%);
  display:block;
  width: 16%;
  height: 12%;
  
}
.top_ul{
  /* background-color:black; */
  position:absolute;
  display: flex;
  /* top:100rpx; */
  left:180rpx;
  height:200rpx;
  width:550rpx;
}
.top_ul li{
  height:60rpx;
  font-size:13px;
  color:rgb(67,67,67);
  line-height:60rpx;
}
.top_ul text{
  font-size:17px;
  transform: translateY(10%);
}
.createtime{
  position:absolute;
  top:70rpx;
  left:0rpx;

}
.num{
  position:absolute;
  top:130rpx;
  left:0rpx;
}
.name{
  position:absolute;
  top:70rpx;
  left:295rpx;
}
.existednum{
  position:absolute;
  top:130rpx;
  left:295rpx;
}
.arrow{
  position:absolute;
  width:20rpx;
  right:10rpx;
  top:80rpx;
}
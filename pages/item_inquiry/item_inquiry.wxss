view{
  font-family: "_GB2312 KaiTi_GB2312";
  color:#444;
  font-weight: 500;
}
.container{
  display:flex;
  justify-content:flex-start;
  padding:0;
}
.nav{
  display:position;
  width:100%;
  /* position:relative;
  bottom:50rpx; */
  height:100rpx;
  background-color:rgb(48,146,247);
  color:white;
  text-align:center;
  line-height:100rpx;
  font-size: 45rpx;
  z-index:100;
}
.nav image{
  position:absolute;
  left:18rpx;
  top:24rpx;
  width:28rpx;
}
.top{
  position:relative;
  background: white;
  width:100%;
  height:80rpx;
  border-bottom:1px solid #ccc;
  z-index:100;
}
.top_content{
  background:rgb(241,241,241);
  position: absolute;
  width:90%;
  height:70rpx;
  top:10rpx;
  left:50%;
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
  transform: translateX(-50%);
}
.logo{
  position:absolute;
  height:50rpx;
  width: 50rpx;
  background: #0BF;
  border-radius: 3px;
  top:50%;
  transform: translateY(-50%);
  left:20rpx;
}
.logo2{
  width:45rpx;
  position:absolute;
  right:20rpx;
  top:50%;
  transform:translateY(-50%);
}
.top_content text{
  position: absolute;
  left:90rpx;
  font-size: 35rpx;
  top:50%;
  transform: translateY(-50%);
}
.top_below{
  position:relative;
  height:300rpx;
  width:100%;
  background: white;
}
.top_below_content{
  position:absolute;
  border-bottom-left-radius: 3px;
  border-bottom-right-radius:3px;
  background: rgb(241,241,241);
  width:90%;
  left:50%;
  transform: translateX(-50%);
  height:100%;
}
.picker_top{
  background:rgb(241,241,241);
  display: flex;
  position:relative;
  width:100%;
  margin:0 auto;
  height:70rpx;
  border-bottom:1px solid #d1d3d4;
  border-top:1px solid #d1d3d4;
  z-index:100;
}
.picker_form{
  width:100%;
  position: relative;
  margin:0 auto;
  height: 230rpx;
  display: none;
}
.picker_form.show{
  display: block;
}
.picker_form.hidden{
  display: none;
}

.picker_form text{
  position: relative;
  display: inline-block;
  font-size:30rpx;
  margin-left: 30rpx;
}
.picker_form input{
  position:absolute;
  left:180rpx;
  top:50%;
  transform: translateY(-50%);
  width:300rpx;
  height:60rpx;
  font-size: 30rpx;
  border: 1px solid #ccc;
  border-radius: 5rpx;
}
.picker_form .name{
  position:relative;
  width:75%;
  height: 100rpx;
  line-height: 100rpx;
}
.picker_form button {
  position: absolute;
  width: 150rpx;
  height: 60rpx;
  right: 30rpx;
  font-size: 30rpx;
  bottom: 50rpx;
  line-height: 60rpx;
}
.picker_form .type{
  position:relative;
  width:75%;
  height: 100rpx;
  line-height: 100rpx;
}
.picker_nav{
  float:left;
  width:33%;
  height:70rpx;
  text-align: center;
  line-height:70rpx;
}
.border{
  border-left:1px solid #d1d3d4;
  border-right:1px solid #d1d3d4;
}
.content{
  display: inline-block;
  font-size:30rpx;
}
.icon{
  margin-left:15rpx;
  display:inline-block;
  border:4px solid transparent;
  border-top:4px solid #9b9b9b;
}
.depr{
  display: inline-block;
  position: relative;
  background:#c5c5c5;
  overflow-y:scroll;
  width:67%;
  height:700rpx;
  line-height: 80rpx;
  color:#717273;
  font-size:30rpx;
}
/* 部门/区县内容栏 */
.dep_content{
  positive:relative;
  width:100%;
  height:700rpx;
  background: white;
  display: none;
}
@keyframes show{
  from{
    transform: translateY(-100%);
  }
  to{
    transform: translateY(0%);
  }
}

.dep_content.show,
.type_name.show{
  display:block;
  animation:show 0.5s  ease-in both;
}

.dep{
  display: inline-block;
  position: relative;
  background:#c5c5c5;
  overflow-y:scroll;
  width:33%;
  height:700rpx;
  line-height: 80rpx;
  color:#717273;
  font-size:30rpx;
}
.right{
  background:#e0e0e0;
}
.dep_region{
  padding-left:10rpx;
  font-size: 28rpx;
  text-align: center;
  overflow-x: true;
  height: 70rpx;
}

/* 类型名称栏 */
.type_name{
  display:none;
  position: relative;
  width:100%;
  background: white;
  overflow-y: scroll;
  color:#666;
}


/* 类型名称栏(头部) */
.tn_top{
  position: relative;
  width:100%;
  height:80rpx;
  line-height:80rpx;
  border-bottom: 1px solid #ccc;
}
.tn_top text{
  position: absolute;
  left:20rpx;
  font-size:32rpx;
}
.t_type{
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  line-height: 60rpx;
  width:200rpx;
  height:60rpx;
  left:300rpx;
  font-size: 28rpx;
  text-align: center;
  border:1px solid #ccc;
  border-radius: 4px;
}
.t_status{
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  line-height: 60rpx;
  width:120rpx;
  height:60rpx;
  left:300rpx;
  font-size: 30rpx;
  text-align: center;
  border:1px solid #ccc;
  border-radius: 4px;
}
.n_name{
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  line-height: 60rpx;
  width:120rpx;
  height:60rpx;
  left:450rpx;
  font-size: 30rpx;
  text-align: center;
  border:1px solid #ccc;
  border-radius: 4px;
}
/* 类型栏的内容 */
.t_content{
  position: relative;
  width:100%;
  /* height:700rpx; */
}
.tn_text{
  display: block;
  position: relative;
  margin-top:10rpx;
  margin-left:20rpx;
  font-size:32rpx;
}
.t_content_middle{
  display: flex;
  position: relative;
  /* margin-top:60rpx; */
  width: 100%;
  justify-content:flex-start;
  flex-wrap:wrap;
  align-content:flex-start;
}
t_content_middle_status{
  display: flex;
  position: relative;
  /* margin-top:60rpx; */
  width: 100%;
  justify-content:flex-start;
  flex-wrap:wrap;
  align-content:flex-start;
}
.t_content_middle_status .content{
  margin-top:20rpx;
  width:140rpx;
  height:60rpx;
  margin-left:20rpx;
  border:1px solid #ccc;
  border-radius: 3px;
  text-align:center;
  line-height: 60rpx;
  font-size: 30rpx;
}
.t_content_middle .content{
  margin-top:20rpx;
  width:200rpx;
  height:60rpx;
  margin-left:20rpx;
  border:1px solid #ccc;
  border-radius: 3px;
  text-align:center;
  line-height: 60rpx;
  font-size: 28rpx;
}

/* 名称栏的内容   其内容样式采用上面类型块*/ 
.n_content{
  margin-top:30rpx;
  border-top:1px solid #ccc;
  position: relative;
  width:100%;
  height:300rpx;
}
.bottom{
  margin: 0 auto;
  position:absolute;
  top:510rpx;
  height:120rpx;
  width:90%;
  background:rgb(241,241,241);
  border-top-left-radius: 3px;
  border-top-right-radius: 3px;
  border-bottom: 1px solid #ccc;
}
.text1{
  color:black;
}
.list{
  position: relative;
  background: rgb(245, 245, 245);
  width: 90%;
  height: 940rpx;
  top: 20rpx;
  border-top-left-radius: 3rpx;
  border-top-right-radius: 3rpx;
  z-index: -1;
}
.item{
  display: block;
  position: relative;
  height: 50rpx;
  line-height: 50rpx;
  border-bottom: 1px solid rgb(230, 230, 230)
}
.item text{
  font-size: 25rpx;
  color: rgb(98, 98, 98);
}

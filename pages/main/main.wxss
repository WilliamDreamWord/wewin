.head{
  position:relative;
  width:100%;
  height:360rpx;
  background-color: rgb(103, 162, 230);   
  
}
.row{
  position:absolute;
  width:100%;
  margin:0 auto;
  height:100rpx;
  top:20rpx;
  border-bottom: 1px solid rgb(169, 169, 169);
  /* background: white; */
  text-align:center;
}
.row text{
  display: block;
  width: 100%;
  top:50%;
  transform: translateY(-50%);
  position:absolute;
  color:white;
  
}
.top{
  position:absolute;
  width:90%;
  height:360rpx;
  top:150rpx;
  left:50%;
  transform: translateX(-50%);
  background-color: rgb(244, 244, 244);  
  border-radius: 5px;
}
.top2{
  position: relative;
  width: 100%;
  height:64rpx;
  border-bottom: 1px solid rgb(220, 220, 220);
  font-size: 28rpx;
}
.icon{
  width:38rpx;
  height:38rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left:15rpx;
  background-color: rgb(103, 163, 230);
  border-radius: 3px;
}
.top2 text{
  position:absolute;
  display: block;
  left:70rpx;
  top: 50%;
  transform: translateY(-50%);
}
.top3{
  position: relative;
  width: 100%;
  height:295rpx;
}
.text1{
  position: relative;
  width: 100%;
  height: 140rpx;
  text-align: center;
}
.totolNum{
  position: relative;
  top:20rpx;
  width: 100%;
  height:70rpx;
  color: rgb(115, 169, 225);
  text-align:center;
  font-size: larger;
}
.text1 text{
  font-size:30rpx;
  color: rgb(76, 76, 76);
}

.data{
  display: flex;
  justify-content:center;
  position:relative;
  top:20rpx;
  width: 100%;
  height:135rpx;
}
.data view{
  font-size:30rpx;
  width:180rpx;
  height:100%;
  text-align:center;
}
.useNum{
  color: rgb(115, 169, 225);
  height:60%;
}
.freeNum{
  color: rgb(115, 169, 225);
  height:60%;
}
.data text{
  color:rgb(76, 76, 76);
}
.newNum{
  color:rgb(76, 76, 76);
  height:60%;
}

.buttom{
  position: absolute;
  top:550rpx;
  left:50%;
  transform: translateX(-50%);
  width:90%;
  height:320rpx;
  border-radius:5px;
  background-color: rgb(244, 244, 244);  
}
.buttom2{
  position: relative;
  width: 100%;
  height:64rpx;
  border-bottom: 1px solid rgb(220, 220, 220);
  font-size: 28rpx;
}
.icon_buttom{
  width:38rpx;
  height:38rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left:15rpx;
  background-color: rgb(103, 163, 230);
  border-radius: 3px;
}
.icon_text{
  position:absolute;
  display: block;
  left:70rpx;
  top: 50%;
  transform: translateY(-50%);
}
.buttom3_data{
  display: flex;
  justify-content:center;
  position:relative;
  top:20rpx;
  width: 100%;
  height:260rpx;
}
.buttom3_data view{
  position: relative;
  top:40rpx;
  width:180rpx;
  height:180rpx;
  text-align: center;
}
.buttom3_data image{
  width:80rpx;
}
.buttom3_data view text{
  position: absolute;
  display: block;
  width:100%;
  margin:0 auto;
  font-size:30rpx;
  text-align:center;
  color: rgb(76, 76, 76);
}

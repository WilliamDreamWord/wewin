.container{
  display:flex;
  justify-content:flex-start;
  padding:0;
}
.nav{
  display:position;
  width:100%;
  /* position:relative;
  bottom:50rpx; */
  height:100rpx;
  background-color:rgb(48,146,247);
  color:white;
  text-align:center;
  line-height:100rpx;
  font-size: 45rpx;
}
.top{
  position:relative;
  width:100%;
  height:80rpx;
  background-color:white;
  line-height:80rpx;
  font-size: 14px;
}
.top image{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 45rpx;
  top: 25rpx;
}
.num{
  position: absolute;
  left:20rpx;
  color: rgb(67,67,67);
}
.existed_num{
  color:#0BF;
  position: absolute;
  left:330rpx;
}
.content{
  margin: 0 auto;
  position:relative;
  height:120rpx;
  width:100%;
  background:white;
}
.content_son{
  position:relative;
  border-radius:4px 4px 0 0;
  height:100rpx;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%);
  background:rgb(241,241,241);
  position:absolute;
  width:90%;
  line-height:100rpx;
  color:rgb(66,66,66);
}
.text1{
  font-weight: 320;
  position:absolute;
  left:20rpx;
}
.pandian{
  font-size:13px;
  display:inline-block;
  border-radius:5px;
  top:50%;
  transform: translateY(-50%);
  width:130rpx;
  height:65rpx;
 
  position: absolute;
  left:520rpx;
  line-height: 65rpx;
  text-align:center;
  color: rgb(255, 255, 255);
}
